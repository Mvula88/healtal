# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# AI Configuration
# Option 1: Replicate (Recommended)
REPLICATE_API_TOKEN=your_replicate_token_here

# Option 2: Anthropic API
ANTHROPIC_API_KEY=your_anthropic_key_here

# Stripe Payment Processing
STRIPE_SECRET_KEY=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_WEBHOOK_SECRET=

# Stripe Price IDs for Subscription Tiers
# Lite Plan - $9/month
NEXT_PUBLIC_STRIPE_LITE_PRICE_ID=price_1S2pxcIVPWhx8miGx7cj9BTc
# Starter Plan - $19/month
NEXT_PUBLIC_STRIPE_STARTER_PRICE_ID=
# Growth Plan - $39/month  
NEXT_PUBLIC_STRIPE_GROWTH_PRICE_ID=
# Premium Plan - $79/month
NEXT_PUBLIC_STRIPE_PREMIUM_PRICE_ID=
# Monitoring
SENTRY_DSN=
SENTRY_AUTH_TOKEN=

# App Configuration
NEXTAUTH_SECRET=generate_random_secret_here
NEXTAUTH_URL=http://localhost:3000

# Crisis Resources
CRISIS_HOTLINE_NUMBER=988
PROFESSIONAL_REFERRAL_API_KEY=your_api_key_here

# Rate Limiting (Required for API protection)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# OpenAI API (Optional - for voice features)
OPENAI_API_KEY=

# VAPID Keys for Push Notifications (Optional)
NEXT_PUBLIC_VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=